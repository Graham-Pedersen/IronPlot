<UserControl x:Class="None.IronRacket_ReplWindow.MyControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             Background="{DynamicResource VsBrush.Window}"
             Foreground="{DynamicResource VsBrush.WindowText}"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="700"
             Name="MyToolWindow">
    <Grid>
        <TextBox Width="700" Height="300" HorizontalAlignment="Center" Text=">" Name="Repl_input_output" AcceptsReturn="True" AcceptsTab="True" Background="LightGray" TextChanged="TextBox_TextChanged" />
        <!-- <Button Content="_Click Me!" Click="button1_Click" Width="120" Height="80" Name="button1"/> -->
    </Grid>
</UserControl>
